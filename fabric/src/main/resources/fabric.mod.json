{
  "schemaVersion": 1,
  "id": "betterarcheology",
  "version": "${version}",
  "name": "${displayName}",
  "description": "${modDescription}",
  "authors": [
    "${authors}"
  ],
  "contact": {
    "issues": "https://github.com/Pandarix/BetterArcheologyMultiLoaded/issues"
  },
  "license": "MIT",
  "icon": "assets/betterarcheology/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.Pandarix.fabric.BAFabric"
    ],
    "client": [
      "net.Pandarix.fabric.client.BAFabricClient"
    ],
    "fabric-datagen": [
      "net.Pandarix.fabric.datagen.ModDataGenerators"
    ]
  },
  "mixins": [
    "betterarcheology.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17.0",
    "minecraft": "~1.21",
    "java": ">=21",
    "architectury": ">=15.0.0",
    "resourcefulconfig": ">=3.0.3",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
